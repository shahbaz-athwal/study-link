{
  "rewrites": [
    {
      "source": "/api/ph/static/:path(.*)",
      "destination": "https://us-assets.i.posthog.com/static/:path*"
    },
    {
      "source": "/api/ph/:path(.*)",
      "destination": "https://us.i.posthog.com/:path*"
    },
    {
      "source": "/((?!api|_next/static|_next/image|favicon.ico|ingest).*)",
      "destination": "/"
    }
  ]
}
